    "start": "blitz start",
    "studio": "blitz prisma studio",
    "typecheck": "tsc --noEmit",
    "export": "blitz build && next export",
    "lint": "next lint",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
