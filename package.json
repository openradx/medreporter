{
  "name": "medreporter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "analyze:view": "http-server ./.next/analyze/",
    "start": "next start",
    "export": "next build && next export",
    "tsc": "tsc --noEmit",
    "lint": "next lint && npm run lint:stylelint",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:unit:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:report": "npm run test:e2e && npm exec playwright show-report",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "ci": "npm run prettier:check && npm run lint && npm run tsc && npm run test",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create-only": "prisma migrate dev --create-only",
    "db:reset": "NODE_ENV=development prisma migrate reset dev",
    "db:seed": "NODE_ENV=development prisma db seed",
    "db:studio": "prisma studio",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "figure:extract": "tsx scripts/extractFigureTranslations.ts",
    "figure:process": "tsx scripts/processFigureTranslations.ts",
    "lng:extract": "lingui extract",
    "lng:clean": "lingui extract --clean",
    "lng:compile": "lingui compile --typescript",
    "pkg:outdated": "npm outdated",
    "pkg:interactive": "ncu --interactive --format group"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
