{
  "name": "medreporter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "analyze:view": "http-server ./.next/analyze/",
    "start": "next start",
    "export": "next build && next export",
    "tc": "tsc --noEmit",
    "lint": "next lint && pnpm run lint:stylelint",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:unit:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:report": "pnpm run test:e2e && playwright show-report",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "ci": "pnpm run prettier:check && pnpm run lint && pnpm run tc && pnpm run test",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create-only": "prisma migrate dev --create-only",
    "db:reset": "NODE_ENV=development prisma migrate reset dev",
    "db:seed": "NODE_ENV=development prisma db seed",
    "db:studio": "prisma studio",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "figure:extract": "tsx scripts/extractFigureTranslations.ts",
    "figure:process": "tsx scripts/processFigureTranslations.ts",
    "pkg:outdated": "pnpm outdated",
    "pkg:interactive": "pnpm update --latest --interactive"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
