{
  "name": "medreporter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "analyze:view": "http-server ./.next/analyze/",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:unit:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:report": "pnpm test:e2e && playwright show-report",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "ci": "pnpm prettier && pnpm lint && pnpm typecheck && pnpm test",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create-only": "prisma migrate dev --create-only",
    "db:reset": "NODE_ENV=development prisma migrate reset dev",
    "db:seed": "NODE_ENV=development prisma db seed",
    "db:studio": "prisma studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "figure:extract": "tsx scripts/extractFigureTranslations.ts",
    "figure:process": "tsx scripts/processFigureTranslations.ts",
    "pkg:outdated": "pnpm outdated",
    "pkg:interactive": "pnpm update --latest --interactive"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
