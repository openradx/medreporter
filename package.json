    "storybook": "start-storybook -p 7001",
    "storybook:build": "build-storybook",
    "svg:options": "node -r @swc-node/register scripts/parseSVG.ts --output options",
    "svg:translations": "node -r @swc-node/register scripts/parseSVG.ts --output translations",
    "i18n:missed": "i18n-unused display-missed",
